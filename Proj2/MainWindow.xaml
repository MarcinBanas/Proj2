<Window x:Class="Proj2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proj2"
        mc:Ignorable="d"
        Title="Projekt" Height="450" Width="818">
    <Grid Margin="10,0,10,0">
        <Button x:Name="ZarządzajAktywami" Content="Zarządzaj Aktywami" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="ZarządzajAktywamiOpenWindow"/>
        <Button x:Name="DodajNotowanieButton" Content="Dodaj Notowanie" HorizontalAlignment="Left" Margin="124,10,0,0" VerticalAlignment="Top" Click="DodajNotowanieOpenWindow"/>
        <DataGrid x:Name="DataGridMain"  Margin="0,91,0,0" ItemsSource="{Binding }" CanUserAddRows="False" ColumnWidth="*"/>
        <Button x:Name="ZaladujAktywaButton" Content="ZaladujAktywa" HorizontalAlignment="Left" Margin="702,30,0,0" VerticalAlignment="Top" Click="ZaladujAktywaButton_Click"/>

    </Grid>
</Window>
